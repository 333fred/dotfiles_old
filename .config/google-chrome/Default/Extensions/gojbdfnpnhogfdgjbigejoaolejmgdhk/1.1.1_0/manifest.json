{
   "background": {
      "scripts": [ "ThirdParty/jquery.1.7.1.js", "OneNote.js" ]
   },
   "browser_action": {
      "default_icon": "Images/icon38.png",
      "name": "Clip to OneNote"
   },
   "content_scripts": [ {
      "css": [ "hideInstallButton.css" ],
      "matches": [ "*://*.onenote.com/Clipper/OneNote", "*://*.onenote-int.com/Clipper/OneNote" ]
   }, {
      "js": [ "ThirdParty/jquery.1.7.1.js", "tooltips/TooltipBootstrapper.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "Images/icon128.png",
      "19": "Images/icon19.png",
      "256": "Images/icon256.png",
      "48": "Images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "OneNote Clipper",
   "permissions": [ "http://*/*", "https://*/*", "contextMenus", "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.1",
   "web_accessible_resources": [ "hideInstallButton.css" ]
}
